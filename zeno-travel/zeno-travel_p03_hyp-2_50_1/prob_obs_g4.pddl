(define	(problem zenotravelproblem_g4)
	(:domain zenotravel)
	(:init
		(at plane1 city2)
		(fuellevel plane1 fl5)
		(at plane2 city2)
		(fuellevel plane2 fl0)
		(at person1 city0)
		(at person2 city1)
		(at person3 city0)
		(at person4 city0)
		(at person5 city2)
		(= (total-cost) 0)
	)
	(:goal
		(and
			(observation4)
			(at plane1 city0)
			(at person1 city2)
			(at person2 city1)
			(at person3 city1)
			(at person4 city1)
			(at person5 city1)
		)
	)
	(:metric minimize (total-cost))
)
